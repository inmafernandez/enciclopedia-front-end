acing: 0.1rem;\r\n}\r\n\r\n.bg-black {\r\n  background-color: #000;\r\n}\r\n\r\n.rounded-pill {\r\n  border-radius: 5rem;\r\n}\r\n\r\n.navbar-custom {\r\n  padding-top: 1rem;\r\n  padding-bottom: 1rem;\r\n  background-color: rgba(72, 120, 217, 0.9);\r\n}\r\n\r\n.navbar-custom .navbar-brand {\r\n  text-transform: uppercase;\r\n  font-size: 1.2rem;\r\n  letter-spacing: 0.1rem;\r\n  font-weight: 700;\r\n}\r\n\r\n.navbar-custom .navbar-nav .nav-item .nav-link {\r\n  text-transform: uppercase;\r\n  font-size: 0.8rem;\r\n  font-weight: 700;\r\n  letter-spacing: 0.1rem;\r\n}\r\n\r\n\r\n/*header background*/\r\nheader.masthead {\r\n  position: relative;\r\n  overflow: hidden;\r\n  padding-top: calc(7rem + 72px);\r\n  padding-bottom: 7rem;\r\n  background: linear-gradient(0deg, #05F2AF 0%, #25C7D9 100%);\r\n  background-repeat: no-repeat;\r\n  background-position: center center;\r\n  background-attachment: scroll;\r\n  background-size: cover;\r\n}\r\nheader.masthead .masthead-content {\r\n  z-index: 1;\r\n  position: relative;\r\n}\r\n\r\nheader.masthead .masthead-content .masthead-heading {\r\n  font-size: 4rem;\r\n}\r\n\r\nheader.masthead .masthead-content .masthead-subheading {\r\n  font-size: 2rem;\r\n}\r\n\r\nheader.masthead .bg-circle {\r\n  z-index: 0;\r\n  position: absolute;\r\n  border-radius: 100%;\r\n  background: linear-gradient(0deg, #25C7D9 0%, #05F2AF 100%);\r\n}\r\n\r\nheader.masthead .bg-circle-1 {\r\n  height: 50rem;\r\n  width: 50rem;\r\n  top: -5rem;\r\n  left: -30rem;\r\n}\r\n\r\nheader.masthead .bg-circle-2 {\r\n  height: 45rem;\r\n  width: 45rem;\r\n  top: -25rem;\r\n  right: 1rem;\r\n}\r\n\r\nheader.masthead .bg-circle-3 {\r\n  height: 30rem;\r\n  width: 30rem;\r\n  bottom: -5rem;\r\n  right: -10%;\r\n}\r\n\r\nheader.masthead .bg-circle-4 {\r\n  height: 30rem;\r\n  width: 30rem;\r\n  top: -5rem;\r\n  right: 45%;\r\n}\r\nheader.masthead .bg-circle-5 {\r\n  height: 20rem;\r\n  width: 20rem;\r\n  bottom: 2rem;\r\n  right: 25%;\r\n}\r\nheader.masthead .bg-circle-6 {\r\n  height: 20rem;\r\n  width: 20rem;\r\n  bottom: 1rem;\r\n  right: 65%;\r\n}\r\n\r\n@media (min-width: 992px) {\r\n  header.masthead {\r\n    padding-top: calc(10rem + 55px);\r\n    padding-bottom: 10rem;\r\n  }\r\n  header.masthead .masthead-content .masthead-heading {\r\n    font-size: 6rem;\r\n  }\r\n  header.masthead .masthead-content .masthead-subheading {\r\n    font-size: 4rem;\r\n  }\r\n}\r\n\r\n/*header carousel*/\r\n.carousel-item {\r\n  height: 100vh;\r\n  min-height: 350px;\r\n  background: no-repeat center center scroll;\r\n  -webkit-background-size: cover;\r\n  -moz-background-size: cover;\r\n  -o-background-size: cover;\r\n  background-size: cover;\r\n}\r\n\r\n.carousel{\r\n  margin-bottom: 50px;\r\n}\r\n\r\n/*---------------------------header post img------------------------*/\r\n\r\n.masthead {\r\n  height: 100vh;\r\n  min-height: 500px;\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n  color:white;\r\n}\r\n\r\n/*---------------------------post text ------------------------*/\r\n\r\n.post {\r\n  padding-top: 30px;\r\n}\r\n\r\n/*---------------------------post blockquote------------------------*/\r\n\r\n.blockquote-custom {\r\n  position: relative;\r\n  font-size: 1.1rem;\r\n  padding-top: 50px;\r\n \r\n}\r\n\r\n.blockquote-custom-icon {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: absolute;\r\n  top: -25px;\r\n  left: 50px;\r\n}\r\n\r\n\r\n/*---------------------------pie de img------------------------*/\r\n\r\n.pie {\r\n  margin-top: 10px;\r\n  background-color:#C0C0C0;\r\n}\r\n.pie p {\r\n  text-align: right;\r\n  padding-right: 20px;\r\n  color: white;\r\n}\r\n\r\n/*---------------------------bajar el video del header------------------------*/\r\n\r\n.bajo {\r\n  margin-top: 100px;\r\n}\r\n\r\n/*---------------------------formato para código en html------------------------*/\r\ncode {\r\n  background-color: #eee;\r\n  display: block;\r\n  padding:20px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n/*call to action*/\r\n.bg-primary {\r\n  background-color: #881DF2;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: #881DF2;\r\n  border-color: #881DF2;\r\n}\r\n\r\n.btn-primary:active, .btn-primary:focus, .btn-primary:hover {\r\n  background-color: #4878D9;\r\n  border-color: #4878D9;\r\n}\r\n\r\n.btn-primary:focus {\r\n  box-shadow: 0 0 0 0.2rem rgba(72, 120, 217, 0.7);\r\n}\r\n\r\n.btn-secondary {\r\n  background-color: #25C7D9;\r\n  border-color: #25C7D9;\r\n}\r\n\r\n.btn-secondary:active, .btn-secondary:focus, .btn-secondary:hover {\r\n  background-color: #05F2C7;\r\n  border-color: #05F2C7;\r\n}\r\n\r\n.btn-secondary:focus {\r\n  box-shadow: 0 0 0 0.2rem rgba(37, 199, 217, 0.5);\r\n}\r\n\r\n\r\n/*footer*/\r\n\r\n.footer {\r\n  padding: 25px 0;\r\n  text-align: center;\r\n  background-color: #4878D9;\r\n  font-family: 'Lato';\r\n  color:white;\r\n}\r\n\r\n.footer span.copyright {\r\n  font-size: 90%;\r\n  line-height: 40px;\r\n  text-transform: none;\r\n}\r\n\r\n\r\nul.social-buttons {\r\n  margin-bottom: 0;\r\n}\r\n\r\nul.social-buttons li a {\r\n  font-size: 20px;\r\n  line-height: 50px;\r\n  display: block;\r\n  width: 50px;\r\n  height: 50px;\r\n  transition: all 0.3s;\r\n  color: #4878D9;\r\n  border-radius: 100%;\r\n  outline: none;\r\n  background-color: white;\r\n}\r\n\r\nul.social-buttons li a:active, ul.social-buttons li a:focus, ul.social-buttons li a:hover {\r\n  background-color: #4878D9;\r\n  color: white;\r\n}\r\n\r\nul.quicklinks li a {\r\ncolor: white;\r\nfont-family: 'Lato';\r\n\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"785647b0cf3346be1666b2fafea004f7","cacheData":{"env":{}}}